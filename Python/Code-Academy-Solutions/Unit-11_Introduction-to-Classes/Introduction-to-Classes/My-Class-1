#9 of 18
class Animal(object):
  """Makes cute animals."""
  is_alive = True
  health = "good"
  def __init__(self, name, age):
    self.name = name
    self.age = age
  # Add your method here!
  def description(self):
    print self.name 
    print self.age
    
hippo = Animal("George", 12)

hippo.description()

sloth = Animal("Pal", 5)
ocelot = Animal("Mystery", 3)

print hippo.health
print sloth.health
print ocelot.health
